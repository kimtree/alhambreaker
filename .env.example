# 2Captcha API Key
# Get your API key from: https://2captcha.com/enterpage
CAPTCHA_API_KEY=your_2captcha_api_key
# Captcha solving timeout in seconds (optional, default: 180)
CAPTCHA_TIMEOUT=180

# Telegram Bot Configuration
# Create a bot via @BotFather and get the token
TELEGRAM_BOT_TOKEN=your_bot_token
# Get your chat ID via @userinfobot or @getidsbot
TELEGRAM_CHAT_ID=your_chat_id

# Target Dates to Monitor (comma-separated YYYY-MM-DD, must be same month)
TARGET_DATES=2026-02-17,2026-02-18,2026-02-20

# Ticket Type (GENERAL, GARDENS, NIGHT, etc.)
TICKET_TYPE=GENERAL

# Browser Settings (optional)
HEADLESS=true
BROWSER_TIMEOUT=30000
